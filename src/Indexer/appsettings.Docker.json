{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Indexer": {
    "Workers":
    [
      {
        "Name": "pythonExample",
        "Script": "Scripts/example.py",
        "Calls": [
          {
            "Type": "interval",
            "Interval": 30000
          }
        ]
      }
    ],
    "ApiKeys": ["APIKeyOfYourChoice", "AnotherOneIfYouLike"],
    "Server": {
      "BaseUri": "http://172.17.0.1:5146",
      "ApiKey": "APIKeyForTheServer"
    },
    "PythonRuntime": "libpython3.13.so"
  }
}
